fun is_prime(n: int) -> bool:
    if (n <= 1):
        return false
    end
    if (n <= 3):
        return true
    end
    if (n % 2 == 0 || n % 3 == 0):
        return false
    end
    i: int = 5
    while (i * i <= n):
        if (n % i == 0 || n % (i + 2) == 0):
            return false
        end
        i += 6
    end
    return true
end

println(is_prime(29)) // true
