fun encode():
    import fs
    import os
    content: str = fs.read_file(__file__)
    print(content.replace("\\", "\\\\").replace("\n", "\\n").replace("\r", "\\r").replace("\t", "\\t").replace("\"", "\\\""))
    os.exit(0)
end

